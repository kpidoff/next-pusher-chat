{
  "name": "@next-pusher-chat/core",
  "version": "1.1.45",
  "private": false,
  "description": "A real-time chat solution for Next.js using Pusher",
  "author": "Votre Nom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-username/next-pusher-chat"
  },
  "keywords": [
    "next.js",
    "pusher",
    "chat",
    "real-time"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsup",
    "dev": "nodemon --watch src --ext ts,tsx --exec \"tsup\"",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.0.0",
    "next": "^14.0.0",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.15.15",
    "@mui/icons-material": "^5.15.15",
    "@emotion/react": ">=11 <12",
    "@emotion/styled": ">=11 <12"
  },
  "dependencies": {
    "@types/lodash": "^4.17.16",
    "emoji-picker-react": "^4.12.2",
    "jose": "^6.0.11",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nodemon": "^3.1.10",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0"
  },
  "workspaces": [
    "."
  ]
}
